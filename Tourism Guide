#include <stdio.h>

int main()
{
    int arr1[30];
    for (int i = 1; i <= 30; i++)
    {
        scanf("%d", &arr1[i]); // scanning arr1
        if (arr1[i] < 0)
        {
            break;
        }
    }
    int n = 0;
    for (int i = 1; i <= 30; i++)
    {
        if (arr1[i] <= 0)
        {
            break;
        } // counting n for arr2
        else
        {
            n++;
        }
    }
    int arr2[n];
    for (int i = 1; i <= n; i++)
    {
        arr2[i] = arr1[i]; // copying arr1 to arr2
    }
    for (int i = 1; i <= n; i++)
    {
        printf("%d ", arr2[i]); // printing arr2
    }
    printf("\n");

    int max = arr2[1];
    for (int i = 2; i <= n; i++)
    {
        if (arr2[i] > max)
        {
            max = arr2[i]; // finding the max value of the arr2
        }
    }

    int count[max];

    for (int i = 1; i <= max; i++)
    {
        count[i] = 0;
        for (int j = 1; j <= n; j++)
        {
            if (arr2[j] == i)
            {
                count[i]++; // counting the repeatations
            }
        }
    }

    for (int i = 1; i <= max; i++)
    {
        if (count[i] > 1)
        {
            printf("Value %d: %d place are deleted\n", i, count[i] - 1);
        }
    }
    for (int i = 1; i <= max; i++)
    {
        if (count[i] >= 1)
        {
            printf("%d ", i); // printing the values
        }
    }

    return 0;
}
